<h1>All Announcements</h1>

<table class="table">
  <tr>
    <th>Title</th>
    <th>Contents</th>
    <th>Date</th>
    <th>Created at</th>
    <th>Last Updated</th>
  </tr>
  <% @announcements.each do |a| %>
    <tr>
      <td><%= a.title %></td>
      <td><%= a.contents %></td>
      <td><%= a.date %></td>
      <td><%= a.created_at %></td>
      <td><%= a.updated_at %></td>
    </tr>
  <% end %>
</table>
